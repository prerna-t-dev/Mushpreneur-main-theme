<section class="min-h-screen">
    <div id="__enzuzo-root"></div>
    <script id="__enzuzo-root-script" src="https://app.enzuzo.com/scripts/privacy/4cb53556-df42-11ef-a187-43a7a8ecc293"></script>
</section>


<script>
    function resetEnzuzoStyles() {
        // Select the Enzuzo root element
        let enzuzoRoot = document.getElementById("__enzuzo-root");

        if (enzuzoRoot) {
            // Remove any inline styles
            enzuzoRoot.removeAttribute("style");

            // Remove all child elements that could be injecting styles
            let styles = enzuzoRoot.getElementsByTagName("style");
            for (let style of styles) {
                style.remove();
            }

            console.log("Enzuzo styles reset.");
        } else {
            console.warn("Enzuzo root not found yet.");
        }
    }

    // Run this function after a delay to allow Enzuzo to load
    setTimeout(resetEnzuzoStyles, 3000); 
</script>
