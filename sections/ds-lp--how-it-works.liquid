    {
      "type": "text",
      "id": "day7_label",
      "label": "Day 7 Label",
      "default": "Day 7"
    },
    {
      "type": "richtext",
      "id": "day7_title",
      "label": "Day 7 Title",
      "default": "<p>Mental Clarity + Stress Support</p>"
    },
    {
      "type": "textarea",
      "id": "day7_description",
      "label": "Day 7 Description",
      "default": "You're thinking clearer. Less reactive. More steady. More grounded. Adaptogens start calming your system, while nootropics like Lion's Mane sharpen focus and flow."
    },
    {
      "type": "text",
      "id": "day30_label",
      "label": "Day 30 Label",
      "default": "Day 30"
    },
    {
      "type": "richtext",
